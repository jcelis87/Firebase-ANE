<?xml version="1.0" encoding="utf-8"?>
<extension xmlns="http://ns.adobe.com/air/extension/19.0">
    <id>com.tuarua.firebase.InvitesANE</id>
    <name>Firebase Invites ANE</name>
    <copyright>This work is licensed under Apache License, Copyright (c) 2017 Tua Rua Ltd.</copyright>
    <versionNumber>0.3.0</versionNumber>
    <platforms>
        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libInvitesANE.a</nativeLibrary>
                <initializer>TRFIRIVExtInizer</initializer>
                <finalizer>TRFIRIVExtFinizer</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="iPhone-x86">
            <applicationDeployment>
                <nativeLibrary>libInvitesANE.a</nativeLibrary>
                <initializer>TRFIRIVExtInizer</initializer>
                <finalizer>TRFIRIVExtFinizer</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="Android-ARM">
            <applicationDeployment>
                <nativeLibrary>classes.jar</nativeLibrary>
                <initializer>com.tuarua.firebase.InvitesANE</initializer>
                <finalizer>com.tuarua.firebase.InvitesANE</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="Android-x86">
            <applicationDeployment>
                <nativeLibrary>classes.jar</nativeLibrary>
                <initializer>com.tuarua.firebase.InvitesANE</initializer>
                <finalizer>com.tuarua.firebase.InvitesANE</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="default">
            <applicationDeployment/>
        </platform>
    </platforms>
</extension>
