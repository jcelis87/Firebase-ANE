<?xml version="1.0" encoding="utf-8"?>
<extension xmlns="http://ns.adobe.com/air/extension/19.0">
    <id>com.tuarua.firebase.AuthANE</id>
    <name>Firebase Auth ANE</name>
	<copyright>This work is licensed under Apache License, Copyright (c) 2017 Tua Rua Ltd.</copyright>
	<versionNumber>0.0.8</versionNumber>
    <platforms>
     <platform name="iPhone-ARM">
        <applicationDeployment>
            <nativeLibrary>libAuthANE.a</nativeLibrary>
            <initializer>TRFIRAUExtInizer</initializer>
            <finalizer>TRFIRAUExtFinizer</finalizer>
        </applicationDeployment>
    </platform>
     <platform name="iPhone-x86">
         <applicationDeployment>
             <nativeLibrary>libAuthANE.a</nativeLibrary>
             <initializer>TRFIRAUExtInizer</initializer>
             <finalizer>TRFIRAUExtFinizer</finalizer>
         </applicationDeployment>
     </platform>
