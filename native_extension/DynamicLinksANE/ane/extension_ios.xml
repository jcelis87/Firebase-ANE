<?xml version="1.0" encoding="utf-8"?>
<extension xmlns="http://ns.adobe.com/air/extension/19.0">
    <id>com.tuarua.firebase.DynamicLinksANE</id>
    <name>Firebase Dynamic Links ANE</name>
	<copyright>This work is licensed under Apache License, Copyright (c) 2017 Tua Rua Ltd.</copyright>
	<versionNumber>0.0.5</versionNumber>
    <platforms>
        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libDynamicLinksANE.a</nativeLibrary>
                <initializer>TRFIRDLExtInizer</initializer>
                <finalizer>TRFIRDLExtFinizer</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="iPhone-x86">
            <applicationDeployment>
                <nativeLibrary>libDynamicLinksANE.a</nativeLibrary>
                <initializer>TRFIRDLExtInizer</initializer>
                <finalizer>TRFIRDLExtFinizer</finalizer>
            </applicationDeployment>
        </platform>
     <platform name="default">
         <applicationDeployment/>
     </platform>
    </platforms>
</extension>
